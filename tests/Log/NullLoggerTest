<?php

namespace Expose\Log;

/**
 * @coversDefaultClass \Expose\Log\NullLogger
 */
class NullLoggerTest extends \PHPUnit_Framework_TestCase
{
    /**
     *
     * @covers ::log
     */
    public function testLog()
    {
        $this->assertTrue(
            (new \Expose\Log\NullLogger)->log('info', 'Info message);
        );
    }
}
